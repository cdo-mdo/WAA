type Book {
    id: ID!
    title: String!
    author: String!
    isbn: String!
    price: Float!
}

type Query {
    books: [Book!]!
    bookById(id: ID!): Book
}

type Mutation {
    addBook(title: String!, author: String!, isbn: String!, price: Float!): Book!
    deleteBook(id: ID!): Boolean!
}
